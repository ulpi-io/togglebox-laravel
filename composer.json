{
  "name": "togglebox/laravel",
  "version": "0.1.0",
  "description": "Laravel SDK for ToggleBox - Remote Config, Feature Flags, and A/B Experiments",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "togglebox",
    "laravel",
    "laravel-package",
    "feature-flags",
    "remote-config",
    "ab-testing",
    "experiments",
    "configuration-management",
    "experimentation",
    "analytics"
  ],
  "homepage": "https://github.com/ulpi-io/togglebox",
  "support": {
    "issues": "https://github.com/ulpi-io/togglebox/issues",
    "source": "https://github.com/ulpi-io/togglebox",
    "docs": "https://github.com/ulpi-io/togglebox/tree/main/packages/sdk-laravel"
  },
  "authors": [
    {
      "name": "ToggleBox Team",
      "email": "support@togglebox.io"
    }
  ],
  "require": {
    "php": "^8.1",
    "togglebox/sdk": "^0.1",
    "illuminate/support": "^10.0|^11.0",
    "illuminate/cache": "^10.0|^11.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "orchestra/testbench": "^8.0|^9.0",
    "phpstan/phpstan": "^1.10"
  },
  "autoload": {
    "psr-4": {
      "ToggleBox\\Laravel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ToggleBox\\Laravel\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "analyse": "phpstan analyse src --level=8"
  },
  "extra": {
    "laravel": {
      "providers": [
        "ToggleBox\\Laravel\\ToggleBoxServiceProvider"
      ],
      "aliases": {
        "ToggleBox": "ToggleBox\\Laravel\\Facades\\ToggleBox"
      }
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
